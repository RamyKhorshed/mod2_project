<h1>Welcome to TrackSuite</h1>
<%= form_tag("/sessions", method: "post") do %>
  <%= label_tag(:username, "Username:") %>
  <%= text_field_tag(:username) %><br>
  <%= label_tag(:password, "Password:") %>
  <%= password_field_tag(:password) %><br>
  <%= submit_tag("submit") %>
<% end %>

 <p> Not registered yet? <a href="/users/new"> Sign up here</a></p>
